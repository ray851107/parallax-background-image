{
  "name": "parallax-background-image",
  "version": "2.3.6",
  "description": "js parallax background-image",
  "type": "module",
  "main": "src/main.js",
  "unpkg": "dist/parallax-background-image.js",
  "scripts": {
    "prettier": "prettier --write README.md demo.html 'src/**/*.js' 'tools/**/*.js'",
    "prebuild": "npm run prettier",
    "build": "esbuild src/main.js --bundle --minify --sourcemap --target=es6 --global-name=parallax --outfile=dist/parallax-background-image.js",
    "preversion": "npm run build",
    "generate-script-tag": "node tools/generate-script-tag.js",
    "version": "npm run generate-script-tag && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ray851107/parallax-background-image.git"
  },
  "author": "ray851107",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ray851107/parallax-background-image/issues"
  },
  "homepage": "https://github.com/ray851107/parallax-background-image#readme",
  "devDependencies": {
    "esbuild": "^0.12.8",
    "prettier": "^1.19.1",
    "ssri": "^8.0.1"
  },
  "dependencies": {}
}
